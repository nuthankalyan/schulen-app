{
  "root": "build/",
  "clean_urls": false,
  "routes": {
    "/main/browseprojects": "index.html",
    "/main/browseprojects/*": "index.html",
    "/**": "index.html"
  },
  "headers": {
    "/**": {
      "Cache-Control": "no-store, no-cache, must-revalidate",
      "Access-Control-Allow-Origin": "*"
    }
  }
} 